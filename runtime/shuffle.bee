: dig
  [swap] dip swap ;

: bury
  swap [swap] dip ;

: over
  [dup] dip swap ;

: rot
  dig ;

: -rot
  bury ;

: nip
  swap pop ;

: tuck
  swap over ;

: 2dup
  [dup] dip dup dig swap ;

: 2pop
  pop pop ;

: 2swap
  todo ;

: 2over
  todo ;

: 2nip
  todo ;

: 2tuck
  todo ;

: 2rot
  todo ;

